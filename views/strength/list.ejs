<h1 class="display-4">Testresultaten</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Shoulder Presses</th>
            <th>Squats</th>
            <th>Pull Ups</th>
            <th>Stabiliteit</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% strength.forEach(strengthRecord => { %>

            <tr>
                <td><%= strengthRecord.id %></td>
                <td><%= strengthRecord.shoulderPressReps %></td>
                <td><%= strengthRecord.squatReps %></td>
                <td><%= strengthRecord.pullUpReps %></td>
                <td><%= strengthRecord.coreStability %></td>
                <td>
                    <a href="/strength/edit/<%= strengthRecord.id %>" class="btn btn-primary">Bewerken</a>
                    <form class="d-inline" action="/strength/delete" method="post">
                        <input type="hidden" name="id" value="<%= strengthRecord.id%>">
                        <input type="submit" value="Verwijder" class="btn btn-danger" />
                    </form>
                </td>
            </tr>

        <% }) %>
    </tbody>
</table>
